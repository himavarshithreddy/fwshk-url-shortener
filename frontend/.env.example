# Backend API URL (leave empty if frontend and backend share the same origin)
REACT_APP_API_URL=

# Base URL for shortened links (defaults to window.location.origin if not set)
REACT_APP_BASE_URL=

# reCAPTCHA v3 site key (optional â€” obtain from https://www.google.com/recaptcha/admin)
# When set, the frontend will load reCAPTCHA v3 and send a token with each shorten request.
# The corresponding secret key must be set as RECAPTCHA_SECRET_KEY in the backend .env.
REACT_APP_RECAPTCHA_SITE_KEY=
